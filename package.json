{
  "name": "q-copy",
  "version": "1.0.0",
  "description": "A CLI tool to copy multiple file contents to system clipboard",
  "main": "dist/index.js",
  "bin": {
    "q-copy": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "install-global": "npm run build && npm install -g ."
  },
  "keywords": [
    "clipboard",
    "copy",
    "files",
    "cli"
  ],
  "author": "Marcus Shepherd",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^18.11.18",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
